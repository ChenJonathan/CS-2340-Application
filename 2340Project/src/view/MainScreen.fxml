<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainScreenController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="1000.0" prefWidth="1500.0">
         <children>
            <TextField alignment="CENTER" editable="false" focusTraversable="false" layoutX="156.0" layoutY="53.0" mouseTransparent="true" text="Main Screen">
               <font>
                  <Font size="96.0" />
               </font>
            </TextField>
            <Button layoutX="703.0" layoutY="474.0" mnemonicParsing="false" onAction="#handleLogoutPressed" text="Log Out" />
            <Button layoutX="855.0" layoutY="474.0" mnemonicParsing="false" onAction="#handleViewReportPressed" text="View Report" />
            <Button layoutX="1003.0" layoutY="473.0" mnemonicParsing="false" onAction="#handleNewReportCreation" text="New Report" />
            <Button fx:id="btnProfile" layoutX="705.0" layoutY="563.0" mnemonicParsing="false" onAction="#handleProfilePressed" text="Profile " />
         
         </children>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Pane>
   </children>
</GridPane>
